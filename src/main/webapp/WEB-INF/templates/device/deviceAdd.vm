<script src="${www}/js/my97DatePicker/WdatePicker.js"></script>

<div class="main-content">
  <div class="breadcrumbs" id="breadcrumbs">
    <script src="${www}/js/jquery.maskedinput.min.js"></script>
    <script type="text/javascript">
      try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
      jQuery(function($) {
        $('.input-mask-date').mask('9999-99-99');
      });
    </script>
    <ul class="breadcrumb">
      <li>
        <i class="icon-home home-icon"></i>
        <a href="${www}/back/main">主页</a>
      </li>
      <li>
        <a href="${www}/device/deviceList">列表</a>
      </li>
      <li class="active">新增设备</li>
    </ul><!-- .breadcrumb -->
  </div>
  <div class="page-content">
    <div class="page-header">
      <h1>新增商户产品</h1>
    </div>
    #if(${msg})
      <div>
        #if(${success})
          <li class="text-success">$!{msg}</li>
        #else
          <li class="text-danger">$!{msg}</li>
        #end
      </div>
    #end
    <form id="inputForm" action="${www}/back/shopProduct/insert" method="post" class="form-horizontal">
      <div class="row">
        <div class="col-xs-12 col-md-8">
          <div class="form-group">
            <label class="col-md-4 col-sm-2 control-label">商家id：</label>
            <div class="col-md-8 col-sm-8">
              <input type="text" class="col-sm-12" id="shopId" name="shopId" value="$!{shopProduct.shopId}">
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12 col-md-8">
          <div class="form-group">
            <label class="col-md-4 col-sm-2 control-label">产品名称：</label>
            <div class="col-md-8 col-sm-8">
              <input type="text" class="col-sm-12" id="name" name="name" value="$!{shopProduct.name}">
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12 col-md-8">
          <div class="form-group">
            <label class="col-md-4 col-sm-2 control-label">产品图片：</label>
            <div class="col-md-8 col-sm-8">
              <input type="text" class="col-sm-12" id="imgUrl" name="imgUrl" value="$!{shopProduct.imgUrl}">
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12 col-md-8">
          <div class="form-group">
            <label class="col-md-4 col-sm-2 control-label">产品小标题：</label>
            <div class="col-md-8 col-sm-8">
              <input type="text" class="col-sm-12" id="tip" name="tip" value="$!{shopProduct.tip}">
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12 col-md-8">
          <div class="form-group">
            <label class="col-md-4 col-sm-2 control-label">借款金额范围(元)：</label>
            <div class="col-md-8 col-sm-8">
              <input type="text" class="col-sm-12" id="loanMoney" name="loanMoney" value="$!{shopProduct.loanMoney}">
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12 col-md-8">
          <div class="form-group">
            <label class="col-md-4 col-sm-2 control-label">月利率(%)：</label>
            <div class="col-md-8 col-sm-8">
              <input type="text" class="col-sm-12" id="rate" name="rate" value="$!{shopProduct.rate}">
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12 col-md-8">
          <div class="form-group">
            <label class="col-md-4 col-sm-2 control-label">天数：</label>
            <div class="col-md-8 col-sm-8">
              <input type="text" class="col-sm-12" id="days" name="days" value="$!{shopProduct.days}">
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12 col-md-8">
          <div class="form-group">
            <label class="col-md-4 col-sm-2 control-label">初始申请人数：</label>
            <div class="col-md-8 col-sm-8">
              <input type="text" class="col-sm-12" id="申请人数" name="申请人数" value="0">
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12 col-md-8">
          <div class="form-group">
            <label class="col-md-4 col-sm-2 control-label">申请条件：</label>
            <div class="col-md-8 col-sm-8">
              <input type="text" class="col-sm-12" id="applyConditon" name="applyConditon" value="$!{shopProduct.applyConditon}">多行条件通过&lt;br&gt;符号分割换行
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12 col-md-8">
          <div class="form-group">
            <label class="col-md-4 col-sm-2 control-label">介绍：</label>
            <div class="col-md-8 col-sm-8">
              <input type="text" class="col-sm-12" id="introduce" name="introduce" value="$!{shopProduct.introduce}">多行介绍通过&lt;br&gt;符号分割换行
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12 col-md-8">
          <div class="form-group">
            <label class="col-md-4 col-sm-2 control-label">上架时间：</label>
            <div class="col-md-8 col-sm-8">
              <input type="text" class="input-date" id="startTime" name="startTime" onFocus="WdatePicker({lang:'zh-cn',dateFmt:'yyyy-MM-dd HH:mm'})"  value="$!{shopProduct.startTime}">
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12 col-md-8">
          <div class="form-group">
            <label class="col-md-4 col-sm-2 control-label">下架时间：</label>
            <div class="col-md-8 col-sm-8">
              <input type="text" class="input-date" id="endTime" name="endTime" onFocus="WdatePicker({lang:'zh-cn',dateFmt:'yyyy-MM-dd HH:mm'})"  value="$!{shopProduct.endTime}">
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12 col-md-8">
          <div class="form-group">
            <label class="col-md-4 col-sm-2 control-label">排序值：</label>
            <div class="col-md-8 col-sm-8">
              <input type="text" class="col-sm-12" id="sortValue" name="sortValue" value="$!{shopProduct.sortValue}">
            </div>
          </div>
        </div>
      </div>
      <div class="clearfix form-actions">
        <div class="row">
          <div class="col-md-8">
            <div class="col-sm-offset-4 col-md-9">
              <button id="sbt" class="btn btn-info" type="submit">
                <i id="iok" class="icon-ok bigger-110">提交</i>
              </button>
            </div>
          </div>
        </div>
      </div>
    </form>
  </div><!-- /.main-content -->
  <script src="${www}/js/youcaicommon.js"></script>